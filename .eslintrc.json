{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "eol-last": "off",
        "linebreak-style": "off",
        "indent": "off",
        "no-alert": "off",
        "no-undef": "off",
        "arrow-body-style": "off",
        "no-unneeded-ternary": "off",
        "no-underscore-dangle": "off",
        "spaced-comment": "off",
        "object-curly-newline": "off",
        "max-len": "off",
        "comma-dangle": "off",
        "operator-linebreak": "off",
        "implicit-arrow-linebreak": "off",
        "quotes": "off"
    }
}
